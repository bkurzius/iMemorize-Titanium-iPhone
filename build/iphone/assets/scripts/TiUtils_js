var TiUtils=function(){var e=Ti.Platform.osname;return{showAlertDialog:function(e,t,i){var o=Titanium.UI.createAlertDialog({title:e,message:t,buttonNames:i});o.show()},isiPad:function(){return"ipad"===e?!0:!1},isAndroid:function(){return"android"===e?!0:!1},isiPhone:function(){return"iphone"===e?!0:!1},isIos:function(){return"iphone"===e||"ipad"===e?!0:!1},getAppHeight:function(){var e;return e=TiUtils.isAndroid()?Ti.Platform.displayCaps.platformHeight:Ti.Platform.displayCaps.platformHeight-App.data.iPhoneStatusBarHeight},getAppWidth:function(){var e;return e=Ti.Platform.displayCaps.platformWidth},getOsName:function(){return Ti.Platform.osname},getOrientation:function(){return Titanium.UI.orientation===Titanium.UI.LANDSCAPE_LEFT||Titanium.UI.orientation===Titanium.UI.LANDSCAPE_RIGHT?"landscape":Titanium.UI.orientation===Titanium.UI.PORTRAIT||Titanium.UI.UPSIDE_PORTRAIT?"portrait":"unknown"},getPickerViewHeight:function(){return"portrait"===TiUtils.getOrientation()?250:200}}}();
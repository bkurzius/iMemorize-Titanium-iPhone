var Ti={_event_listeners:[],createEventListener:function(e){var t={listener:e,systemId:-1,index:this._event_listeners.length};return this._event_listeners.push(t),t},getEventListenerByKey:function(e,t){for(var i=0;i<this._event_listeners.length;i++)if(this._event_listeners[i][e]==t)return this._event_listeners[i];return null},API:TiAPI,App:{addEventListener:function(e,t){var i=Ti.createEventListener(t);return i.systemId=TiApp.addEventListener(e,i.index),i.systemId},removeEventListener:function(e,t){if("number"==typeof t){TiApp.removeEventListener(e,t);var i=Ti.getEventListenerByKey("systemId",t);null!==i&&Ti._event_listeners.splice(i.index,1)}else i=Ti.getEventListenerByKey("listener",t),null!==i&&(TiApp.removeEventListener(e,i.systemId),Ti._event_listeners.splice(i.index,1))},fireEvent:function(e,t){TiApp.fireEvent(e,JSON.stringify(t))}},executeListener:function(e,t){var i=this.getEventListenerByKey("index",e);null!==i&&i.listener.call(i.listener,t)}},Titanium=Ti;